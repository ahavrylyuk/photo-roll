{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
        "indent": ["error", 2],
        "comma-dangle": ["error", "never"],
        "semi": ["error", "never"],
        "react/prefer-stateless-function": 0
    }
}
